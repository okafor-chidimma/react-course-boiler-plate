{
  "name": "boiler-plate",
  "version": "1.0.0",
  "description": "Indecision application for React Beginners",
  "main": "index.js",
  "author": "Okafor Chidimma",
  "license": "MIT",
  "scripts": {
    "serve": "live-server public",
    "start": "node server/server.js",
    "start:dev": "webpack-dev-server --env development",
    "build-babel-watch": "babel src/app.js --out-file=public/scripts/app.js --presets=env,react --watch",
    "build-webpack-watch": "webpack --watch",
    "build:dev": "webpack --env development",
    "build:prod": "webpack -p --env production",
    "heroku-postbuild": "yarn run build:prod",
    "test:configure": "jest",
    "test": "jest --config=jest.config.json",
    "test:configure:watch": "cross-env NODE_ENV=test jest --config=jest.config.json --watchAll",
    "test:watch-mode": "jest --watchAll"
  },
  "dependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^3.2.0",
    "express": "^4.17.3",
    "firebase": "^7.6.1",
    "history": "^4.10.1",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "normalize.css": "^8.0.1",
    "numeral": "^2.0.6",
    "raf": "^3.4.1",
    "react": "^16.10.2",
    "react-dates": "^21.5.0",
    "react-dom": "^16.10.2",
    "react-modal": "^3.10.1",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "uuid": "^3.3.3",
    "webpack": "^4.41.0"
  },
  "devDependencies": {
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "jest": "^24.9.0",
    "live-server": "^1.2.1",
    "mini-css-extract-plugin": "^0.9.0",
    "react-test-renderer": "^16.12.0",
    "redux-mock-store": "^1.5.4",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
